{
  "name": "purple-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel serve:container serve:auth serve:todo",
    "serve:container": "npm run start --prefix ./container",
    "serve:auth": "npm run start --prefix ./auth",
    "serve:todo": "npm run start --prefix ./todo",
    "install:container": "cd ./container && npm i",
    "install:auth": "cd ./auth && npm i",
    "install:todo": "cd ./todo && npm i",
    "postinstall": "npm-run-all --parallel install:container install:auth install:todo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
